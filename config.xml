<?xml version='1.0' encoding='utf-8'?>
<widget id="com.airanza.apass2" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>APass2</name>
    <icon src="www/img/a_icon_white_key_trans_border_96x96.png" />
    <description>
        APass Secure Password Manager.
    </description>
    <author email="info@airanza.com" href="http://www.airanza.com">
        Airanza Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" src="www/res/ios/Icon.png" width="57" />
        <icon height="114" src="www/res/ios/Icon@2x.png" width="114" />
        <icon height="60" src="www/res/ios/icon-60.png" width="60" />
        <icon height="72" src="www/res/ios/Icon-72.png" width="72" />
        <icon height="144" src="www/res/ios/Icon-72@2x.png" width="144" />
        <icon height="50" src="www/res/ios/Icon-Small-50.png" width="50" />
        <icon height="100" src="www/res/ios/Icon-Small-50@2x.png" width="100" />
        <splash height="480" src="www/res/ios/Splash.png" width="320" />
        <splash height="960" src="www/res/ios/Splash@2x.png" width="640" />
        <splash height="1024" src="www/res/ios/Splash-ipad-portrait768x1024.png" width="768" />
        <splash height="2048" src="www/res/ios/Splash-ipad-portrait1536x2048@2x.png" width="1536" />
        <splash height="768" src="www/res/ios/Splash-ipad-landscape1024x768.png" width="1024" />
        <splash height="1536" src="www/res/ios/Splash-ipad-landscape2048x1536@2x.png" width="2048" />
        <splash height="1136" src="www/res/ios/Splash-568h@2x.png" width="640" />
        <splash height="1334" src="www/res/ios/Splash-667h.png" width="750" />
        <splash height="2208" src="www/res/ios/Splash-736h.png" width="1242" />
        <splash height="1242" src="www/res/ios/Splash-landscape736h.png" width="2208" />
    </platform>
    <preference name="FadeSplashScreenDuration" value="1000" />
    <preference name="SplashScreenDelay" value="0" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-device" spec="~1.1.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.2" />
    <plugin name="cordova-plugin-dialogs" spec="~1.2.0" />
    <plugin name="cordova-sqlite-storage" spec="~0.7.14" />
    <plugin name="cordova-plugin-email" spec="~1.1.0" />
    <plugin name="cordova-plugin-file" spec="~4.1.0" />
    <plugin name="cordova-plugin-globalization" spec="~1.0.2" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.3.0" />
    <plugin name="cordova-plugin-admobpro" spec="~2.16.1" />
    <plugin name="cc.fovea.cordova.purchase" spec="~5.0.2">
        <variable name="BILLING_KEY" value="" />
    </plugin>
    <plugin name="cordova-plugin-splashscreen" spec="~3.2.2" />
</widget>
